<h1>Listing Prototypes</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>

      <th></th>
    </tr>
  </thead>
  <tbody>
<%= for prototype <- @prototypes do %>
    <tr>
      <td><%= prototype.name %></td>

      <td>
        <%= link "Show", to: Routes.mud_prototype_path(@conn, :show, @slug, prototype) %>
        <%= link "Edit", to: Routes.mud_prototype_path(@conn, :edit, @slug, prototype) %>
        <%= link "Delete", to: Routes.mud_prototype_path(@conn, :delete, @slug, prototype), method: :delete, data: [confirm: "Are you sure?"] %>
      </td>
    </tr>
<% end %>
  </tbody>
</table>

<span><%= link "New Prototype", to: Routes.mud_prototype_path(@conn, :new, @mud_slug) %></span>
